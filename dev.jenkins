pipeline { 
agent{label'JDK-ALL-MAVEN'}
stages {
	stage('git cloneing') {
		steps {
		git url: 'https://github.com/shopizer-ecommerce/shopizer' , branch: 'devloper'
		 
		}
	    }
	
   stage('build')  {
    steps {
                sh 'mvn package'
            }
        }
        stage('test results') {
            steps {
               junit testResults: '**/surefire-reports/*.xml'
        }
            }
        stage('archive artifacts') {
            steps {
                archiveArtifacts '**/target/*.jar'
            }
        }   
            
       
 }
  }